#User function Template for python3

class Solution:
	def reverseDigits(self, n):
	    result=0
		while(n>0):
		    var=n%10
		    result=result*10+var
		    n=n//10
		return result


#{ 
 # Driver Code Starts
# Initial Template for Python 3

if __name__ == '__main__':
    T = int(input())
    for i in range(T):
        n = int(input())
        ob = Solution()
        ans = ob.reverseDigits(n)
        print(ans)

        print("~")

# } Driver Code Ends
